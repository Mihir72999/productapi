
{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
      {
        "src": "./build/prisma.html",
         "use": "@vercel/static"
      }
  ],
  "routes": [
    {
     
      "src": "(/*.)",
     
      "dest": "index.ts"
    },
     {
            "src": "/",
            "dest": "./build/prisma.html"
        }
  ]
}
