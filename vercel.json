
{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@now/node"
    },
      {
            "src": "build/**",
            "use": "@vercel/static"
      }
  ],
  "routes": [
    {
     
      "src": "(.*)",
     
      "dest": "index.ts"
    },
     {
            "src": "",
            "dest": "build/prisma.html"
        }
  ]
}
