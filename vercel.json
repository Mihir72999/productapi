
{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
  ],
  "routes": [
    {
     
      "src": "/(.*)",
     
      "dest": "index.js"
    },
     {
            "src": "/",
            "dest": "build/prisma.html"
        }
  ]
}
